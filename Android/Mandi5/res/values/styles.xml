<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.

    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.

        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="ActionBarCompat">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@color/white</item>
    </style>
    <style name="mandi5_screen_header_bg">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/header_bg</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="mandi5_screen_back">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/back</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <style name="mandi5_screen_menu">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/menu</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <style name="standard_header_text_view">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/dashboard_list_text_size</item>
    </style>
    <style name="layout_wrap_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="layout_wrap_fill">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="layout_fill_wrap">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="layout_fill_fill_vertical">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="layout_fill_wrap_vertical">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="standard_layout1">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/background</item>
    </style>
    <style name="standard_layout6">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0dip</item>
    </style>
    <style name="standard_layout7">
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="layout_fil_wrap_hori">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="standard_text_wrap_wrap_color">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
    </style>
    
    <style name="standard_textview1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/disclaimer_text_view_size</item>
        <item name="android:textStyle">normal</item>
    </style>
    
    <style name="mandi5_screen_spinner_layout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">@dimen/form_row_parent_padding_top</item>
        <item name="android:paddingBottom">@dimen/form_row_parent_padding_bottom</item>
    </style>
    
    <style name="mandi5_screen_spinner">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:padding">@dimen/spinner_padding</item>
    </style>
    <style name="mandi5_screen_innerbody_layout_style_mar_pad">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/standard_margin10</item>
        <item name="android:padding">@dimen/standard_padding5</item>
     
    </style>
     <style name="mandi5_screen_scroll_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fadingEdge">none</item>
        <!-- <item name="android:overScrollMode">never</item> -->
        <item name="android:fadeScrollbars">false</item>
    </style>
    <style name="standard_edittext1">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/input_bg</item>
        <item name="android:drawablePadding">2dip</item>
        <item name="android:paddingLeft">5dip</item>
        <item name="android:paddingRight">5dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="mandi5_screen_btn_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">40dip</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
    </style>
    <style name="mandi5_text_heading1">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">@dimen/standard_text_size20</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="mandi5_screen_go_btn_style1">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
    </style>
    <style name="layout_fill_fill">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="layout_wrap_wrap_vertical">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>
    

    
    
    
    
    
    
    
    
    
    
    
    <style name="custom_layout1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    
    
    
    
    
   
<!-- Archana end -->
</resources>